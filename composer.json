{
    "name": "guiatrend/guiatrend-nextjs",
    "description": "Guía Turística de México - Next.js Application",
    "type": "project",
    "scripts": {
        "post-install-cmd": [
            "npm install"
        ],
        "post-update-cmd": [
            "npm install"
        ],
        "build": "npm run build",
        "start": "npm start"
    },
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {},
    "config": {
        "optimize-autoloader": true,
        "platform-check": false
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    }
}